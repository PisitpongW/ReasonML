rescript = 1
rule cp
  command = cmd.exe /C copy /Y $i $out >NUL
rule touch
 command = cmd.exe /C type nul >>$out & copy $out+,, >NUL
o RandomColorRe.cmi : cp ..\bs\src\RandomColorRe.cmi
o RandomColorRe.cmj : cp ..\bs\src\RandomColorRe.cmj
o RandomColorRe.cmt : cp ..\bs\src\RandomColorRe.cmt
o RandomColorRe.re : cp ..\..\src\RandomColorRe.re
build install.stamp : touch RandomColorRe.cmi RandomColorRe.cmj 
